<body id="signin-type-2">
    <div class="container-fluid">
        <div id="body-container">
            <div class="standalone-page">
                <div class="standalone-page-logo">
                    <a href="#">
                        @*<img src ="~/Content/img/indir (1).png" width="156" height="44"  />*@
                        @*<img src="~/Content/img/indir.png" width="156" height="44" />*@
                        @*<img src="~/Content/img/indirYeni.png" width="377" height="200" />*@
                              <img src="~/Content/img/indirYeni.png" width="377" height="200" />
                    </a>
                </div>
                <div class="error-messages hidden"></div>
                <div class="standalone-page-content" data-border-top="multi">
                    <div class="standalone-page-block">
                        <div class="row">
                            <div class="col-xs-12">
                                <h2 class="heading">
                                    <span aria-hidden="true" class="icon icon-key"></span>
                                    <span class="main-text">
                                      Giriş Yap
                                    </span>
                                </h2>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                @*<p class="text-center text-primary">
            <span class="label label-success">Tip</span>
            Try submiting the form without filling it in.
        </p>*@
                                <form role="form" class="login-form form-horizontal" method="post" action="/Member/Login">
                                    <div class="form-group">
                                        <label for="inputEmail" class="col-sm-3 control-label">Kullanıcı:</label>
                                        <div class="col-sm-9">
                                            <input autocomplete="off" class="form-control" id="inputEmail" placeholder="Kullanıcı Adı" type="text" name="UsersName">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword" class="col-sm-3 control-label">Şifre</label>
                                        <div class="col-sm-9">
                                            <input autocomplete="off" class="form-control" id="inputPassword" placeholder="Şifre" type="password" name="UsersPsw">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-offset-3 col-sm-9">
                                            <div class="checkbox">
                                                <label>
                                                    <input type="checkbox" name="RememberMe">  Beni Hatırla
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-offset-3 col-sm-9">
                                            <button id="submit-form" type="submit" class="btn btn-success"> Giriş Yap</button>
                                            <a href="/Member/ForgotPsw" class="btn btn-link btn-sm pull-right">Şifremi Unuttum</a>
                                        </div>
                                    </div>
                                </form>

                                @if (ViewBag.Mesaj != null)
                                {
                            <p style="color:red">  @ViewBag.Mesaj </p>
                                        }
                                 
                                </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-12">
                                <div class="change-section">
                                    <h3 class="heading">Kayıt Olmadınız Mı?</h3>
                                    <a href="/Users/UsersAdd" class="btn btn-default btn-block">Kayıt Ol</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div><!-- /.container -->
    <!-- Placed at the end of the document so the pages load faster -->
