
@{
    ViewBag.Title = "Üyeler";
}

@using System.Web.Security
@model MembershipUserCollection

    <div class="row">
        <div class="col-md-12">
            <div class="block">
                <div class="block-heading">
                    <div class="main-text h2">
                        Üyeler          
                        <a href="/Users/UsersAdd" class="btn btn-success rounded">Yeni Üye</a>
                        <a class="btn btn-primary Role" href="/Users/AssignRole/">Rolleri</a>
                    </div>


                </div>
                <div class="block-content-outer">
                    <div class="block-content-inner">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Kullanıcı Adı</th>
                                    <th>Email</th>
                                    <th>Kayıt Olunan Tarihi</th>
                                    <th class="text-center">Son Giriş Tarihi</th>
                                    <th>Rolleri</th>
                                   
                                    <th>Sil</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (MembershipUser u in Model)
                                {
                                <tr>
                                    <td>@u.UserName"</td>
                                    <td>@u.Email</td>
                                    <td>@u.CreationDate</td>
                                    <td>@u.LastLoginDate</td>
                                    <td><a class="btn btn-primary Role" data-UserName="@u.UserName">Rolleri</a></td>

                                    <td><a href="#" class="btn btn-danger  rounded usersDel" data-ajax="@u.UserName">Sil</a></td>

                                </tr>
                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

