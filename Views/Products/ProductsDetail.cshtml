
@{
    ViewBag.Title = "Ürün Detay";
}

@using Hunerli.Models
@model Products

    <div class="row">
        <div class="col-md-12">
            <div class="block">
                <div class="block-heading">
                    <div class="main-text h2">
                        Ürünler             <a href="/Products/ProductsAdd" class="btn btn-success rounded">Ekle</a>
                    </div>


                </div>
                <div class="block-content-outer">
                    <div class="block-content-inner">
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Tedarikçi</th>
                                    <th>Ürün Adı</th>
                                    <th>Stok</th>
                                    <th class="text-center">Status</th>
                                    <th>Fiyatı</th>
                                    <th>Sil</th>

                                </tr>
                            </thead>
                            <tbody>
                           
                                
                                    <tr>
                                        <td>@Model.SellerID</td>
                                        <td>@Model.ProductName</td>
                                        <td>@Model.ProductStocks</td>
                                        <td>@Model.exisT</td>
                                        <td>@Model.ProductCast</td>
                                        <td><a href="#" class="btn btn-danger  rounded productDlt" data-ajax="@Model.ProductID">Sil</a></td>

                                    </tr>
                                

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

