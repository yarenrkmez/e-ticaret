
@{
    ViewBag.Title = "CookieA";
}

    <div class="row">
        <div class="col-md-12">


            <div class="block">
                <div class="block-heading">
                    <div class="main-text h2">
                        Cookie ye Deger Atama 
                    </div>

                </div>
                <div class="block-content-outer">
                    <div class="block-content-inner">
                        <form action="/Home/CookieA" method="post" role="form">
                            <div class="form-group">
                                <label for="exampleInputEmail1">Cookie Adı:</label>
                                <input type="text" class="form-control" id="exampleInputEmail1" placeholder="Cookie Adı Gir" name="CookieName">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputPassword1">Cookie Değeri:</label>
                                <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Cookie Degeri Gir" name="CookieValue">
                            </div>
                       
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </form>
                    </div>
                </div>
            </div>



        </div>

    </div>
